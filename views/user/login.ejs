<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/header.ejs') %>
  </head>
  <body>
    <h1>Login Page</h1>
    <div>
      <form action="/user/login" method="post">
        <label>
          <h2>Enter Email</h2>
          <input type="email" name="email" id="email" />
        </label>
        <label>
          <h2>Enter Password</h2>
          <input type="password" name="password" id="password" />
        </label>
        <button type="submit">Login</button>
      </form>
    </div>
  </body>
</html>
